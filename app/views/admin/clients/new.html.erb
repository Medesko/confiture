<hr>
<h2>
  Nouveau
</h2>
<p></p>
<hr>
<div class="row">
  <div class="col-md-8">
  <div class="content" data-slug="panel1">
  <%= form_for :clients, url: {action:'create'}, :html => { :multipart => true }, :class=> "form-horizontal  form-border no-margin" do |f| %>
    <% if @clients.errors.any? %>
      <div id="error_explanation">
        <ul>
        <% @clients.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
      <% end %>
      <div class="form-group">
        <label for=""><%= f.label :name %></label>
        <%= f.text_field :name, :class =>"form-control"%>
      </div>
      <div class="form-group">
        <label for=""><%= f.label :tel %></label>
        <%= f.text_field :tel, :class =>"form-control" %>
      </div>
      <div class="form-group">
        <label for=""><%= f.label :email %></label>
        <%= f.email_field :email, :class =>"form-control" %>
      </div>
      <div class="form-group">
        <label for=""><%= f.label :address %></label>
        <%= f.text_field :address, :class =>"form-control" %>
      </div>
      <%= f.submit "Publier", :class=> "btn btn-default" %>
    <% end %>
  </div>
  </div>
</div>